{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:seed:test": "tsx scripts/seed-test-data.ts",
    "db:seed:e2e": "tsx scripts/seed-e2e-data.ts",
    "db:seed:performance": "tsx scripts/seed-performance-data.ts",
    
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:all": "npm run lint && npm run type-check && npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:performance",
    "test:unit": "vitest run tests/unit --reporter=verbose --reporter=json --outputFile=test-results/unit-results.json",
    "test:integration": "vitest run tests/integration --reporter=verbose --reporter=json --outputFile=test-results/integration-results.json",
    "test:e2e": "playwright test tests/e2e --reporter=html",
    "test:performance": "playwright test tests/performance --reporter=html",
    "test:a11y": "playwright test tests/accessibility --reporter=html --output-dir=a11y-results",
    "test:cross-browser": "playwright test tests/e2e --project=chromium --project=firefox --project=webkit",
    "test:smoke:staging": "playwright test tests/smoke --grep=@staging",
    "test:smoke:production": "playwright test tests/smoke --grep=@production",
    
    "test:watch": "vitest watch tests/unit tests/integration",
    "test:coverage": "vitest run tests/unit tests/integration --coverage --reporter=json --outputFile=coverage/coverage-summary.json",
    "test:coverage:html": "vitest run tests/unit tests/integration --coverage --reporter=html",
    "test:coverage:lcov": "vitest run tests/unit tests/integration --coverage --reporter=lcov",
    
    "test:report": "node scripts/test-report-generator.js",
    "test:report:open": "npm run test:report && open test-reports/test-report.html",
    
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    
    "playwright:install": "npx playwright install --with-deps",
    "playwright:test": "npx playwright test",
    "playwright:ui": "npx playwright test --ui",
    "playwright:debug": "npx playwright test --debug",
    "playwright:codegen": "npx playwright codegen",
    
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    
    "ci:test": "npm run lint:check && npm run type-check && npm run test:coverage && npm run test:e2e && npm run test:performance",
    "ci:report": "npm run test:report && npm run test:coverage:lcov"
  },
